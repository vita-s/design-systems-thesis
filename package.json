{
  "name": "@sennder/plankton",
  "version": "1.12.0",
  "scripts": {
    "test": "npm run test:unit",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "vue-cli-service lint",
    "lint:style": "npx stylelint 'src/**/*.(vue|scss|css)'",
    "prebuild": "node ./src/generator.js",
    "build:test-utils": "tsc -p ./src/test-utils/tsconfig.json && tsc -p ./src/test-utils/cypress/tsconfig.json",
    "build": "vue-cli-service build --target lib -name plankton ./src/index.ts && npm run build:test-utils",
    "postbuild": "node ./after-build.js",
    "start": "npm-run-all --parallel watch:build watch:server",
    "watch:build": "vue-cli-service build --watch --formats umd --target lib -name plankton ./src/index.ts",
    "watch:server": "webpack serve --mode development --entry ./src/index.js",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook -s ./src/assets",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook -s ./src/assets",
    "test:unit": "vue-cli-service test:unit -c jest.config.js",
    "unit": "npm run test:unit",
    "unit:watch": "npm run test:unit -- --watch",
    "check:types": "tsc -p ./tsconfig.json && tsc --noEmit true -p ./src/test-utils/tsconfig.json && tsc --noEmit true -p ./src/test-utils/cypress/tsconfig.json"
  },
  "peerDependencies": {
    "@vue/composition-api": "1.0.0-beta.2",
    "vue": "2.6.12"
  },
  "dependencies": {
    "axios-mock-adapter": "1.18.1",
    "bootstrap-vue": "2.15.0",
    "dropzone": "5.5.1",
    "filesize": "6.1.0",
    "material-design-icons": "3.0.1",
    "moment": "2.22.2",
    "text-mask-addons": "3.8.0",
    "tiny-cookie": "2.3.1",
    "uuid": "3.3.3",
    "v-tooltip": "2.0.3",
    "vue-js-toggle-button": "1.3.2",
    "vue-multiselect": "2.1.6",
    "vue-router": "3.0.7",
    "vue-text-mask": "6.1.2",
    "vue2-daterange-picker": "0.6.3",
    "vuex": "3.1.3",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/plugin-proposal-optional-chaining": "7.12.16",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/gitlab": "6.0.9",
    "@semantic-release/npm": "7.0.10",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@storybook/addon-actions": "6.1.21",
    "@storybook/addon-docs": "6.1.21",
    "@storybook/addon-knobs": "6.1.21",
    "@storybook/addon-links": "6.1.21",
    "@storybook/addon-notes": "5.3.21",
    "@storybook/addon-storysource": "6.1.21",
    "@storybook/vue": "6.1.21",
    "@testing-library/jest-dom": "5.10.1",
    "@types/cypress": "1.1.3",
    "@types/jest": "26.0.16",
    "@types/lodash": "4.14.165",
    "@types/testing-library__jest-dom": "5.9.5",
    "@typescript-eslint/eslint-plugin": "4.9.0",
    "@typescript-eslint/parser": "4.9.0",
    "@vue/cli-plugin-babel": "4.5.11",
    "@vue/cli-plugin-eslint": "4.5.11",
    "@vue/cli-plugin-typescript": "4.5.11",
    "@vue/cli-plugin-unit-jest": "4.5.11",
    "@vue/cli-service": "4.5.11",
    "@vue/composition-api": "1.0.0-beta.2",
    "@vue/eslint-config-prettier": "6.0.0",
    "@vue/eslint-config-typescript": "7.0.0",
    "@vue/test-utils": "1.0.3",
    "axios": "0.19.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "conventional-changelog-cli": "2.1.1",
    "conventional-changelog-conventionalcommits": "4.5.0",
    "eslint": "6.8.0",
    "eslint-plugin-html": "6.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "6.2.2",
    "git-branch": "2.0.1",
    "glob": "7.1.6",
